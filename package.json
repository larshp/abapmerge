{
  "name": "abapmerge",
  "version": "0.11.2",
  "description": "Merge ABAP INCLUDEs into single file",
  "bin": {
    "abapmerge": "./abapmerge"
  },
  "main": "build/merge.js",
  "scripts": {
    "build": "tsc --pretty",
    "buildw": "tsc -w --pretty",
    "testw": "mocha --recursive --watch build/test",
    "pretest": "tsc --pretty && mocha --recursive --reporter progress build/test",
    "test": "tslint ./src/*.ts ./test/*.ts",
    "lint": "tslint ./src/*.ts ./test/*.ts",
    "posttest": "browserify build/src/web.js -o web/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larshp/abapmerge.git"
  },
  "keywords": [
    "ABAP"
  ],
  "author": "Lars Hvam Petersen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larshp/abapmerge/issues"
  },
  "homepage": "https://github.com/larshp/abapmerge#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.10.4",
    "browserify": "^16.2.1",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
